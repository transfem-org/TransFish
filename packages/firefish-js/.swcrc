{
	"$schema": "https://json.schemastore.org/swcrc",
	"jsc": {
		"parser": {
			"syntax": "typescript",
			"dynamicImport": true,
			"decorators": true
		},
		"transform": {
			"legacyDecorator": true,
			"decoratorMetadata": true
		},
		"target": "es2022"
	},
	"minify": false,
	"module": {
		"type": "commonjs",
		"strict": true
	}
}
